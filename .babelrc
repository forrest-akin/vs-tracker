{
  "plugins": ["transform-decorators-legacy", "transform-regenerator"],

  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "modules": false,
      "loose": true
    }],
    "stage-1",
    "react"
  ],

  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
